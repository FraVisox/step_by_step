<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_maps"
    app:startDestination="@id/start_activity_fragment">
    <fragment
        android:id="@+id/start_activity_fragment"
        android:name="com.example.room.fragments.maps.StartWorkoutFragment"
        android:label="fragment_start"
        tools:layout="@layout/start_activity_maps" >
        <action
        android:id="@+id/action_startToFinish"
        app:destination="@id/finish_activity_fragment" />
    </fragment>
    <fragment
        android:id="@+id/finish_activity_fragment"
        android:name="com.example.room.fragments.maps.FinishWorkoutFragment"
        android:label="fragment_finish"
        tools:layout="@layout/finish_activity_maps" >
        <action
            android:id="@+id/action_finishToStart"
            app:destination="@id/start_activity_fragment" />
    </fragment>
</navigation>